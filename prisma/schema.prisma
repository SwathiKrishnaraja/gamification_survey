generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ExitCandidates {
  id   Int    @id @default(autoincrement())
  mode String
}

model Mode {
  id   Int    @id @default(autoincrement())
  mode String
}

model Participants {
  id                   Int    @id @default(autoincrement())
  survey_mode          String
  result               Json?
  char_count           Json?
  average_time         Int?
  time_taken           Int?
  badges               Int?
  points               Int?
  leaderboard_position Int?
  narratives_unlocked  Int?
  browser              String
}
