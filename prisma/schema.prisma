generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ExitCandidates {
  id Int @default(autoincrement()) @id
}

model Mode {
  id   Int    @default(autoincrement()) @id
  mode String
}

model Participants {
  id                   Int    @default(autoincrement()) @id
  survey_mode          String
  result               Json?
  char_count           Json?
  average_time         Int?
  time_taken           Int?
  badges               Int?
  points               Int?
  leaderboard_position Int?
  narratives_unlocked  Int?
}
